class Solution148 {
    public ListNode148 sortList(ListNode148 head) {
        
    	
    	
    	
    	return head;
    }
    
    public ListNode148 split(ListNode148 head) {
    	
    	if (head==null||head.next==null) {
			return head;
		}
    	
		ListNode148 fast=head;
		ListNode148 slow=head;
		ListNode148 temp=null;
		
		while(fast!=null) {
			
			fast=fast.next;
			if(fast!=null) {
				fast=fast.next;
			}
			
			slow=slow.next;
			
		}
		temp=slow;
		slow=slow.next;
		temp.next=null;
		
		
		
		
		
		
		
		
		return slow;
	}
    
    
    
    
    
}

class ListNode148 {
    int val;
    ListNode148 next;
    ListNode148(int x) { val = x; }
    
    public void print() {
   	 print(this);
    }
    
    public void print(ListNode148 root) {
		while (root!=null) {
			System.out.print(root.val+"->");
			root=root.next;
		}
		System.out.println();
	}
    
    
}