class Solution60 {

	int countk = 0;
	int countn = 1;
	int k;
	int n;
	String ans = "";

	public String getPermutation(int n, int k) {
		this.k = k;
		this.n = n;

		backtrack(1, countk);

		return "";
	}

	public void backtrack(int countn, int countk) {
		System.out.println("backtrack");
		if (countn == n) {
			if (countk == k) {
				System.out.println("find");
				return;
			}
//			countk++;
		}
		for (int i = countn+1; i < n; i++) {

			countn++;
			countk++;
			backtrack(countn, countk);
			countn--;
			countk--;

		}

	}

}